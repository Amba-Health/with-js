    "ava": "^3.3.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "browser-env": "^3.3.0",
    "eslint": "^6.2.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "terser": "^4.6.3"
