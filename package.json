{
  "name": "@cookieshq/with-js",
  "version": "0.0.1",
  "description": "A declarative way to add/remove attributes, classes and styles on HTML elements when JS is loaded.",
  "main": "dist/with-js.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=10.19.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint '**/*.js'",
    "test": "ava",
    "prebuild": "babel src/index.js -o dist/with-js.js --source-maps=inline",
    "build": "terser dist/with-js.js -o dist/with-js.min.js --source-map 'content=inline'",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "author": "CookiesHQ <hello@cookieshq.co.uk>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-modules-umd": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "ava": "^3.3.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "browser-env": "^3.3.0",
    "eslint": "^6.2.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.2",
    "terser": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cookieshq/with-js"
  },
  "contributors": [
    "Romaric Pascal <hello@romaricpascal.is> (https://romaricpascal.is)"
  ]
}
